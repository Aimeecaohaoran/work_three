<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:background="@mipmap/wx">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_142"
        android:orientation="vertical"
        android:background="@drawable/shape"
        android:paddingLeft="@dimen/dp_24"
        android:paddingRight="@dimen/dp_24"
        >


        <TextView
            android:id="@+id/nickname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_60"
            android:text="Aimee"
            android:gravity="center"
            android:textSize="@dimen/sp_14"
            android:textColor="#666666"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_70"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <ImageView
                android:id="@+id/aaa"
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/my_icon_address_n_xhdpi"
                />
            <TextView
                android:id="@+id/personaldata"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="个人资料"
                android:textSize="@dimen/sp_14"
                android:textColor="#666666"
                />

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@drawable/shape"
            android:layout_marginLeft="@dimen/dp_45"
            android:layout_marginRight="@dimen/dp_24"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/my_icon_circle_n_xhdpi"
                />
            <TextView
                android:id="@+id/mycircle"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="我的圈子"
                android:textSize="@dimen/sp_14"
                android:textColor="#666666"
                />

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@drawable/shape"
            android:layout_marginLeft="@dimen/dp_45"
            android:layout_marginRight="@dimen/dp_24"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/my_icon_foot_n_xhdpi"
                />
            <TextView
                android:id="@+id/footprint"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="我的足迹"
                android:textSize="@dimen/sp_14"
                android:textColor="#666666"
                />

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@drawable/shape"
            android:layout_marginLeft="@dimen/dp_45"
            android:layout_marginRight="@dimen/dp_24"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:orientation="horizontal"
            android:gravity="center"
            >
            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/my_icon_wallet_n_xhdpi"
                />
            <TextView
                android:id="@+id/Wallet"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="我的钱包"
                android:textSize="@dimen/sp_14"
                android:textColor="#666666"
                />

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@drawable/shape"
            android:layout_marginLeft="@dimen/dp_45"
            android:layout_marginRight="@dimen/dp_24"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_marginTop="@dimen/dp_20"
            >
            <ImageView
                android:layout_width="@dimen/dp_24"
                android:layout_height="@dimen/dp_24"
                android:background="@mipmap/quanbudd_ddf"
                />
            <TextView
                android:id="@+id/shippingaddress"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="我的收货地址"
                android:textSize="@dimen/sp_14"
                android:textColor="#666666"
                />

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="@drawable/shape"
            android:layout_marginLeft="@dimen/dp_45"
            android:layout_marginRight="@dimen/dp_24"
            />
    </LinearLayout>
    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/mine_icon"
        android:layout_width="@dimen/dp_80"
        android:layout_height="@dimen/dp_80"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/dp_106"
        android:background="@mipmap/a"
        app:roundAsCircle="true"
        app:layout_constraintHorizontal_bias="0.46"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</RelativeLayout>